<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Gestão Clientes</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <script src="https://kit.fontawesome.com/dc1a1422df.js" crossorigin="anonymous"></script>
    <style>
        
    </style>
</head>
<body>
    
    <table class="table table-striped table-bordered table-hover">
        <thead>
          <tr>
            <th scope="col">NOME COMPLETO</th>
            <th scope="col">ENDEREÇO</th>
            <th scope="col">AÇÕES</th>
          </tr>
        </thead>
        <tbody class="table-group-divider">
          <% clientes.forEach(cliente => { %>
            <tr>
              <td><%= cliente.nome_cliente %> <%= cliente.sobrenome_cliente %> </td>
              <td><%= cliente.rua %>, <%= cliente.bairro %>, <%= cliente.cidade %></td>
              <td>
                <a  href="/dashboard/gestao-cliente/view/<%= cliente.id_cliente %>"><i class="fa-solid fa-eye"></i></a>
                <a href=""><i class="fa-solid fa-pen-to-square"></i></a>
                <a href="/dashboard/gestao-cliente/delete/<%= cliente.cpf_cliente %>"><i class="fa-solid fa-trash"></i></a>
              </td>
            </tr>
          <% }) %>
        </tbody>
      </table>

      <a class="btn btn-primary" href="/dashboard"> VOLTAR</a>


    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js" integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.min.js" integrity="sha384-BBtl+eGJRgqQAUMxJ7pMwbEyER4l1g+O15P+16Ep7Q9Q+zqX6gSbd85u4mG4QzX+" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
</body>
</html>